{
  "name": "firebolt",
  "displayName": "firebolt",
  "expo": {
    "name": "firebolt",
    "slug": "firebolt",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.firebolt.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
